<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="RoundDotProgressBar">
        <attr name="CycleFinishColor" format="reference|color"></attr>
        <attr name="CycleDefaultColor" format="reference|color"></attr>
    </declare-styleable>

    <declare-styleable name="LongPressPauseView">
        <attr name="speed" format="integer" />
    </declare-styleable>

    <declare-styleable name="RunningSpeedProgress">
        <attr name="ProgressColor" format="reference|color" />
        <attr name="ProgressBackgroundColor" format="reference|color" />
        <attr name="ProgressTextSize" format="reference|dimension" />
    </declare-styleable>


    <declare-styleable name="CustomImageView">
        <attr name="borderRadius" format="dimension" />
        <attr name="type">
            <enum name="circle" value="0" />
            <enum name="round" value="1" />
        </attr>
        <attr name="src" format="reference" />
    </declare-styleable>


</resources>